{
  "gitbook": ">= 3.0.0",
  "root": "docs",
  "title": "CUI Framework",
  "abstract": "Covisint user interface framework",
  "variables": {
    "portal-page-url": "/learn/cui",
    "portal-display-priority": "900",
    "styleguide" : {
      "siteUrl": "http://cui-styleguide.run.covisintrnd.com"
    }
  },
  "plugins": ["-sharing",
    "include","collapsible-menu","edit-link","bootstrap-callout",
    "prism","-highlight","mermaid-compat@0.2.0-beta1"],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/covisint/cui-gitbook/edit/master/docs",
      "label": "Suggest Edits"
    }
  }
}
